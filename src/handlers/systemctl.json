{
  "actions": ["restart"],
  "name": "Linux Service",
  "description": "Linux service handled by `systemctl`",
  "key": "systemctl",
  "image": "data:image/png;base64,",
  "version": "1.0.0",
  "wrapper": {
    "inputs": [
      {
        "key": "serviceName",
        "name": "Service name",
        "type": {
          "name": "text",
          "regularExpression": { "flags": [], "expression": "\\w{1,100}" }
        },
        "required": true,
        "description": "The name of the service"
      }
    ]
  },
  "source": {
    "name": "flowice-handler-systemctl",
    "type": "npm"
  },
  "installed": false
}
